tiers :: [Nat3->Nat2]  =
  [ [ \x -> case x of
              0 -> 0
              1 -> 0
              2 -> 0
    ]
  , [ \x -> case x of
              0 -> 0
              1 -> 0
              2 -> 1
    , \x -> case x of
              0 -> 0
              1 -> 1
              2 -> 0
    , \x -> case x of
              0 -> 1
              1 -> 0
              2 -> 0
    ]
  , [ \x -> case x of
              0 -> 0
              1 -> 1
              2 -> 1
    , \x -> case x of
              0 -> 1
              1 -> 0
              2 -> 1
    , \x -> case x of
              0 -> 1
              1 -> 1
              2 -> 0
    ]
  , [ \x -> case x of
              0 -> 1
              1 -> 1
              2 -> 1
    ]
  ]

map length (tiers :: [[ Nat3->Nat2 ]])  =  [1,3,3,1]

length (list :: [ Nat3->Nat2 ])  =  8

allUnique (list :: [ Nat3->Nat2 ])  =  True

ratioRepetitions (list :: [ Nat3->Nat2 ])  =  0 % 1
