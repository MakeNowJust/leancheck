map length (tiers :: [[ [()]->[()] ]])  =  [1,2,4,8,...]

length (list :: [ [()]->[()] ])  =  Infinity

allUnique (list :: [ [()]->[()] ])  =  False

ratioRepetitions (list :: [ [()]->[()] ])  =  1 % 3

tiers :: [[()]->[()]]  =
  [ [\_ -> []]
  , [ \_ -> [()]
    , \_ -> []
    ]
  , [ \_ -> [(),()]
    , \x -> case x of
            [] -> []
            _ -> [()]
    , \_ -> []
    , \x -> case x of
            [] -> [()]
            _ -> []
    ]
  , [ \_ -> [(),(),()]
    , \x -> case x of
            [] -> []
            _ -> [(),()]
    , \x -> case x of
            [] -> []
            [()] -> []
            _ -> [()]
    , \_ -> []
    , \x -> case x of
            [()] -> [()]
            _ -> []
    , \_ -> [()]
    , \x -> case x of
            [] -> [()]
            _ -> []
    , \x -> case x of
            [] -> [(),()]
            _ -> []
    ]
  , ...
  ]
