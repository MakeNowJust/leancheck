map length (tiers :: [[ Bool->Maybe Bool ]])  =  [2,8,8]

length (list :: [ Bool->Maybe Bool ])  =  Infinity

allUnique (list :: [ Bool->Maybe Bool ])  =  False

ratioRepetitions (list :: [ Bool->Maybe Bool ])  =  1 % 2

tiers :: [Bool->Maybe Bool]  =
  [ [ \_ -> Nothing
    , \_ -> Nothing
    ]
  , [ \x -> case x of
            False -> Nothing
            True -> Just False
    , \x -> case x of
            False -> Nothing
            True -> Just True
    , \x -> case x of
            False -> Just False
            True -> Nothing
    , \x -> case x of
            False -> Just True
            True -> Nothing
    , \x -> case x of
            False -> Just False
            True -> Nothing
    , \x -> case x of
            False -> Just True
            True -> Nothing
    , \x -> case x of
            False -> Nothing
            True -> Just False
    , \x -> case x of
            False -> Nothing
            True -> Just True
    ]
  , [ \_ -> Just False
    , \x -> case x of
            False -> Just False
            True -> Just True
    , \x -> case x of
            False -> Just True
            True -> Just False
    , \_ -> Just True
    , \_ -> Just False
    , \x -> case x of
            False -> Just True
            True -> Just False
    , \x -> case x of
            False -> Just False
            True -> Just True
    , \_ -> Just True
    ]
  ]
